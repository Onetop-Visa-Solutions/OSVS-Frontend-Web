{
  "name": "osvs",
  "version": "1.4.0",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "run-s clean build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "vite build",
    "serve": "serve -s dist -p 3000",
    "ssr:dev": "jiti server.ts",
    "ssr:build": "run-s clean build:update-bulma-colors ssr:build:*",
    "ssr:build:client": "vite build --ssrManifest --outDir dist/client",
    "ssr:build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "ssr:serve": "cross-env NODE_ENV=production jiti server.ts",
    "ssg:build": "pnpm build:update-bulma-colors && cross-env NODE_ENV=production jiti build-ssg.ts",
    "ssg:serve": "serve dist -p 3000",
    "clean": "rimraf dist",
    "test": "run-p test:*",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-s lint:eslint lint:stylelint",
    "lint": "run-s lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:prettier": "prettier --check \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:prettier:fix": "prettier --write \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:eslint": "eslint --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix"
  },
  "dependencies": {
    "@iconify/iconify": "3.1.1",
    "@nuxt/devalue": "2.0.2",
    "@vueform/slider": "2.1.7",
    "@vueuse/components": "10.3.0",
    "@vueuse/core": "10.3.0",
    "@vueuse/integrations": "10.3.0",
    "axios": "1.4.0",
    "bulma": "0.9.4",
    "cobe": "0.6.3",
    "compression": "1.7.4",
    "date-fns": "2.30.0",
    "mapbox-gl": "2.15.0",
    "nprogress": "0.2.0",
    "pinia": "2.1.6",
    "plyr": "3.7.8",
    "prism-theme-vars": "0.2.4",
    "prismjs": "1.29.0",
    "tslib": "2.6.1",
    "universal-cookie": "4.0.4",
    "vue": "3.3.4",
    "vue-accessible-color-picker": "4.1.4",
    "vue-demi": "0.14.5",
    "vue-i18n": "9.2.2",
    "vue-marquee-text-component": "2.0.1",
    "vue-my-photos": "3.0.1",
    "vue-prism-component": "2.0.0",
    "vue-scrollto": "2.20.0",
    "vue3-carousel": "0.3.1",
    "vue3-markdown-it": "1.0.10",
    "vue3-popper": "1.5.0",
    "vue3-scroll-spy": "1.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@cssninja/bulma-css-vars": "0.9.2",
    "@iconify/json": "2.2.98",
    "@intlify/unplugin-vue-i18n": "0.12.2",
    "@types/compression": "^1.7.2",
    "@types/mapbox-gl": "2.7.12",
    "@types/node": "18.14.0",
    "@types/nprogress": "0.2.0",
    "@types/serve-static": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "@unhead/addons": "^1.2.2",
    "@unhead/ssr": "^1.2.2",
    "@unhead/vue": "^1.2.2",
    "@vitejs/plugin-vue": "4.2.3",
    "autoprefixer": "10.4.14",
    "commitlint": "17.6.7",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-frontmatter": "0.0.8",
    "eslint-plugin-md": "1.0.19",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-vue": "9.16.1",
    "eslint-plugin-vuejs-accessibility": "2.1.0",
    "esno": "0.17.0",
    "fast-glob": "^3.3.1",
    "h3": "1.7.1",
    "jiti": "1.19.1",
    "lint-staged": "13.2.3",
    "listhen": "1.2.2",
    "npm-run-all": "4.1.5",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.6",
    "prettier": "3.0.1",
    "rimraf": "5.0.1",
    "sass": "1.64.2",
    "serve": "^14.2.0",
    "serve-static": "1.15.0",
    "stylelint": "15.10.2",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "10.0.0",
    "stylelint-prettier": "4.0.2",
    "stylelint-scss": "5.0.1",
    "typescript": "5.1.6",
    "unhead": "^1.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-fonts": "^1.0.3",
    "unplugin-icons": "0.16.5",
    "unplugin-vue-components": "0.25.1",
    "vite": "4.4.8",
    "vite-plugin-pages": "0.31.0",
    "vite-plugin-pages-sitemap": "^1.6.1",
    "vite-plugin-purge-icons": "0.9.2",
    "vite-plugin-pwa": "0.16.4",
    "vite-plugin-radar": "0.9.1",
    "vue-router": "4.2.4",
    "vue-tsc": "1.8.8",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint"
    ],
    "*.md": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/markdown-it",
        "postcss",
        "stylelint"
      ]
    }
  }
}